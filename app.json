{
  "expo": {
    "name": "Kidoo",
    "slug": "kidoo-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "kidooapp",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kidooapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon-dark.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kidooapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/splash-icon-dark.png"
          }
        }
      ],
      "./plugins/with-dark-mode-icon.js",
      "./plugins/with-dark-mode-splash.js"
    ]
  }
}
